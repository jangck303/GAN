ence (t-n, ... t-1)\r\n","\tfor i in range(n_in, 0, -1):\r\n","\t\tcols.append(df.shift(i))\r\n","\t\tnames += [('var%d(t-%d)' % (j+1, i)) for j in range(n_vars)]\r\n","\r\n","\t# forecast sequence (t, t+1, ... t+n)\r\n","\tfor i in range(0, n_out):\r\n","\t\tcols.append(df.shift(-i))\r\n","\t\tif i == 0:\r\n","\t\t\tnames += [('var%d(t)' % (j+1)) for j in range(n_vars)]\r\n","\t\telse:\r\n","\t\t\tnames += [('var%d(t+%d)' % (j+1, i)) for j in range(n_vars)]\r\n","\t# put it all together\r\n","\tagg = concat(cols, axis=1)\r\n","\tagg.columns = names\r\n","\t# drop rows with NaN values\r\n","\r\n","\tif dropnan:\r\n","\t\tagg.dropna(inplace=True)\r\n","\treturn agg\r\n"],"execution_count":7,"outputs":[]},{"cell_type":"code","metadata":{"id":"VWXWKvbewiqy","executionInfo":{"status":"ok","timestamp":1615519255322,"user_tz":-540,"elapsed":1182,"user":{"displayName":"장관종","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GiKXJdHfTogGKBDx-jW02buaLjdVUBmrVWznTq2=s64","userId":"03647263347629950645"}}},"source":["# load dataset\r\n","dataset = read_csv('/content/drive/MyDrive/Colab Notebooks/kumdan_2019v1.csv', header=0, index_col=0)\r\n","values = dataset.values"],"execution_count":8,"outputs":[]},{"cell_type":"code","metadata":{"id":"yHESE_b9wlRZ","executionInfo":{"status":"ok","timestamp":1615519255323,"user_tz":-540,"elapsed":1178,"user":{"displayName":"장관종","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GiKXJdHfTogGKBDx-jW02buaLjdVUBmrVWznTq2=s64","userId":"03647263347629950645"}}},"source":["#integer encode direction\r\n","#encoder = LabelEncoder()\r\n","#values[:,4] = encoder.fit_transform(values[:,4])\r\n"],"execution_count":9,"outputs":[]},{"cell_type":"code","metadata":{"id":"XkIryeucwlqT","executionInfo":{"status":"ok","timestamp":1615519255324,"user_tz":-540,"elapsed":1173,"user":{"displayName":"장관종","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GiKXJdHfTogGKBDx-jW02buaLjdVUBmrVWznTq2=s64","userId":"03647263347629950645"}}},"source":["# ensure